{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".next/**", "!.next/cache/**", "dist/**"]
		},
		"lint": {
			"dependsOn": ["^lint"],
			"inputs": ["$TURBO_DEFAULT$"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"test": {
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": []
		}
	}
}
